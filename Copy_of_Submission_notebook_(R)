      },
      "source": [
        "\n",
        "# Variable 3 pol_coverage\n",
        "train_data$pol_coverage<-as.integer(revalue(train_data$pol_coverage, c('Min'=0, 'Med1'=1, 'Med2'=2, 'Max'=3)))\n",
        "str(train_data$pol_coverage)\n",
        "\n",
        "# Variable 7 pol_pay_freq\n",
        "train_data$pol_pay_freq<-as.integer(revalue(train_data$pol_pay_freq, c('Monthly'=0, 'Quarterly'=1, 'Biannual'=2, 'Yearly'=3)))\n",
        "unique(train_data$pol_pay_freq)\n",
        "\n",
        "\n",
        "# Variable 8 pol_payd\n",
        "train_data$pol_payd<-as.integer(revalue(train_data$pol_payd, c('No'=0, 'Yes'=1)))\n",
        "unique(train_data$pol_payd)\n",
        "\n",
        "# Variable 9 pol_usage\n",
        "train_data$pol_usage<-as.integer(revalue(train_data$pol_usage, c('WorkPrivate'=0, 'Retired'=1, 'Professional'=2, 'AllTrips'=3)))\n",
        "unique(train_data$pol_usage)\n",
        "\n",
        "# Variable  10 drv_sex1\n",
        "train_data$drv_sex1<-as.integer(revalue(train_data$drv_sex1, c('M'=0, 'F'=1)))\n",
        "unique(train_data$drv_sex1)\n",
        "\n",
        "# Variable 13 drv_drv2\n",
        "train_data$drv_drv2<-as.integer(revalue(train_data$drv_drv2, c('No'=0, 'Yes'=1)))\n",
        "unique(train_data$drv_drv2)\n",
        "\n",
        "# Variable 14 drv_sex2\n",
        "train_data$drv_sex2<-as.integer(revalue(train_data$drv_sex2, c('M'=0, 'F'=1 , \"0\"=2)))\n",
        "unique(train_data$drv_sex2)\n",
        "head(train_data)\n",
        "\n",
        "# Variable 15 drv_age2\n",
        "for (i in 1:nrow(train_data)){\n",
        "  if(is.na(train_data$drv_age2[i])){\n",
        "    train_data$drv_age2[i] <- as.integer(c(\"0\"), na.rm=TRUE) \n",
        "  }\n",
        "}\n",
        "\n",
        "unique(train_data$drv_age2)\n",
        "class(train_data$drv_age2)\n",
        "\n",
        "# Variable 16 drv_age_lic2\n",
        "for (i in 1:nrow(train_data)){\n",
        "  if(is.na(train_data$drv_age_lic2[i])){\n",
        "    train_data$drv_age_lic2[i] <- as.integer(c(\"0\"), na.rm=TRUE) \n",
        "  }\n",
        "}\n",
        "\n",
        "unique(train_data$drv_age_lic2)\n",
        "class(train_data$drv_age_lic2)\n",
        "\n",
        "\n",
        "unique(train_data$vh_age)\n",
        "head(train_data)\n",
        "\n",
        "# Variable 17 vh_age\n",
        "for (i in 1:nrow(train_data)){\n",
        "  if(is.na(train_data$vh_age[i])){\n",
        "    train_data$vh_age[i] <- as.integer(c(\"0\"), na.rm=TRUE) \n",
        "  }\n",
        "}\n",
        "class(train_data$vh_age)\n",
        "unique(train_data$vh_age)\n",
        "head(train_data)\n",
        "\n",
        "\n",
        "# Variable vh_fuel\n",
        "train_data$vh_fuel<-as.integer(revalue(train_data$vh_fuel, c('Gasoline'=0, 'Diesel'=1 , \"Hybrid\"=2)))\n",
        "unique(train_data$vh_fuel)\n",
        "head(train_data)\n",
        "\n",
        "# Variable vh_type\n",
        "train_data$vh_type<-as.integer(revalue(train_data$vh_type, c('Tourism'=0, 'Commercial'=1)))\n",
        "unique(train_data$vh_type)\n",
        "head(train_data)\n",
        "\n",
        "summary(is.na(train_data))\n",
        "\n",
        "\n",
        "# Variable 21 vh_speed\n",
        "for (i in 1:nrow(train_data)){\n",
        "  if(is.na(train_data$vh_speed[i])){\n",
        "    train_data$vh_speed[i] <- as.integer(c(\"0\"), na.rm=TRUE) \n",
        "  }\n",
        "}\n",
        "class(train_data$vh_speed)\n",
        "unique(train_data$vh_speed)\n",
        "head(train_data)\n",
        "\n",
        "\n",
        "# Variable 21 vh_value\n",
        "for (i in 1:nrow(train_data)){\n",
        "  if(is.na(train_data$vh_value[i])){\n",
        "    train_data$vh_value[i] <- as.integer(c(\"0\"), na.rm=TRUE) \n",
        "  }\n",
        "}\n",
        "class(train_data$vh_value)\n",
        "unique(train_data$vh_value)\n",
        "head(train_data)\n",
        "\n",
        "\n",
        "# Variable 22 vh_weight\n",
        "for (i in 1:nrow(train_data)){\n",
        "  if(is.na(train_data$vh_weight[i])){\n",
        "    train_data$vh_weight[i] <- as.integer(c(\"0\"), na.rm=TRUE) \n",
        "  }\n",
        "}\n",
        "class(train_data$vh_weight)\n",
        "unique(train_data$vh_weight)\n",
        "head(train_data)\n",
        "str(train_data)\n",
        "\n",
        "# vh_make_model_frequency \n",
        "data.test2 <- group_by(train_data,vh_make_model)%>%\n",
        "  summarise(vh_make_model_frequency=n())\n",
        "train_data<-merge(train_data,data.test2,by = \"vh_make_model\")\n",
        "\n",
        "\n",
        "#drv_age1 & drv_age_lic1\n",
        "train_data$drv_age1_bin = cut(train_data$drv_age1,breaks = c(0,25,30,40,50,60,70,80,90,110,Inf),right = FALSE) # This \"cuts\" DriverAge into bins\n",
        "train_data$drv_age_lic1_bin = cut(train_data$drv_age_lic1,breaks = c(0,25,30,40,50,60,70,80,90,110,Inf),right = FALSE) # This \"cuts\" DriverLicAge into bins\n",
        "\n",
        "train_data$drv_age1_bin<-as.integer(revalue(train_data$drv_age1_bin, c(\"[0,25)\"=0, \"[25,30)\"=1 , \"[30,40)\"=2 , \"[40,50)\"=3 , \"[50,60)\"=4, \"[60,70)\"=5, \"[70,80)\"=6 , \"[80,90)\"=7 , \"[90,110)\"=8 )))\n",
        "train_data$drv_age_lic1_bin<-as.integer(revalue(train_data$drv_age_lic1_bin, c(\"[0,25)\"=0, \"[25,30)\"=1 , \"[30,40)\"=2 , \"[40,50)\"=3 , \"[50,60)\"=4, \"[60,70)\"=5, \"[70,80)\"=6 , \"[80,90)\"=7 , \"[90,110)\"=8 )))\n",
